<?xml version="1.0" encoding="utf-8"?>
<data>
    <view id='menu_resource' name='菜单-资源' model='ir.ui.menu'>
        <resource>
            <script src="/res/org/jdoo/base/statics/js/base/menu.js"></script>
            <script src="/res/org/jdoo/base/statics/js/base/model-lookup.js"></script>
        </resource>
    </view>

    <view id='menu_search' name='菜单-查询' model='ir.ui.menu'>
        <search>
            <field name="name"/>
            <field name="model"/>
        </search>
    </view>
    <view id='menu_grid' name='菜单-表格' model='ir.ui.menu'>
        <grid>
            <toolbar buttons="default">
            </toolbar>
            <field name="name"/>
            <field name="model"/>
            <field name="view"/>
            <field name="url"/>
        </grid>
    </view>
    <view id='menu_form' name='菜单-表单' model='ir.ui.menu'>
        <form tree="1" sort_field="sequence">
            <toolbar buttons="default">
            </toolbar>
            <field name="name"/>
            <field name="parent_id" readonly="1" link="0"/>
            <field name="sequence"/>
            <field name="icon"/>
            <field name="model" editor="model_lookup"/>
            <field name="view" editor="menu_view"/>
            <field name="url" colspan="4"/>
            <field name="click" colspan="2"/>
            <field name="css"/>
            <field name="active"/>
        </form>
    </view>
</data>
