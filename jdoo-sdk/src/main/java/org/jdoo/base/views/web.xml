<?xml version="1.0" encoding="utf-8"?>
<data>
    <web id="web_login" name="web.login">
        <html lang="en">
            <head>
                <meta charset="utf-8"/>
                <meta name="viewport" content="width=device-width, initial-scale=1"/>
                <title>{{TITLE}}</title>
                <link rel="stylesheet" href="/org/jdoo/base/statics/plugins/fontawesome-free/css/all.min.css"/>
                <link rel="stylesheet" href="/org/jdoo/base/statics/plugins/icheck-bootstrap/icheck-bootstrap.min.css"/>
                <link rel="stylesheet" href="/org/jdoo/base/statics/dist/css/adminlte.min.css"/>
            </head>
            <body class="hold-transition login-page">
                <noscript>抱歉，你的浏览器不支持 JavaScript</noscript>
                <div class="login-box">
                    <div class="card card-outline card-primary">
                        <div class="card-header text-center">
                            <h1 class="m-0">
                                <b>
                                    <t>JDOO</t>
                                </b>
                            </h1>
                        </div>
                        <div class="card-body">
                            <form id="loginForm">
                                <div class="form-group mb-3">
                                    <div class="input-group">
                                        <input name="account" type="input" class="form-control" placeholder="账号/手机号/邮箱"/>
                                        <div class="input-group-append">
                                            <div class="input-group-text">
                                                <span class="fas fa-envelope"></span>
                                            </div>
                                        </div>
                                    </div>
                                </div>
                                <div class="form-group mb-3">
                                    <div class="input-group">
                                        <input name="password" type="password" class="form-control" placeholder="密码"/>
                                        <div class="input-group-append">
                                            <div class="input-group-text">
                                                <span class="fas fa-lock"></span>
                                            </div>
                                        </div>
                                    </div>
                                </div>
                                <div class="mb-3">
                                    <button name="loginSubmit" type="submit" class="btn btn-primary btn-block">
                                        <t>登录</t>
                                    </button>
                                    <span name="errorMessage" class="text-danger"></span>
                                </div>
                                <div class="row">
                                    <div class="col-8">
                                        <div class="icheck-primary">
                                            <input type="checkbox" id="remember"/>
                                            <label for="remember">
                                                <t>记住密码</t>
                                            </label>
                                        </div>
                                    </div>
                                </div>
                            </form>
                            <div class="social-auth-links text-center mt-2 mb-3">
                                <div class="info-box mb-3 bg-success">
                                    <span class="info-box-icon">
                                        <i class="far fa-heart"></i>
                                    </span>
                                    <div class="info-box-content">
                                        <t>科技造福人类，创新引领未来</t>
                                    </div>
                                </div>
                            </div>

                            <p class="mb-1">
                                <a role="forgot-password" href="#">
                                    <t>忘记密码</t>
                                </a>
                            </p>
                            <p class="mb-0">
                                <a role="register" href="#" class="text-center">
                                    <t>注册</t>
                                </a>
                            </p>
                        </div>
                    </div>
                </div>

                <script src="/org/jdoo/base/statics/plugins/jquery/jquery.min.js"></script>
                <script src="/org/jdoo/base/statics/plugins/bootstrap/js/bootstrap.bundle.min.js"></script>
                <script src="/org/jdoo/base/statics/plugins/jquery-validation/jquery.validate.min.js"></script>
                <script src="/org/jdoo/base/statics/plugins/jquery-validation/additional-methods.min.js"></script>
                <script src="/org/jdoo/base/statics/dist/js/adminlte.min.js"></script>
                <script src="/org/jdoo/base/statics/js/src/util.js"></script>
                <script src="/org/jdoo/base/statics/js/login.js"></script>
            </body>
        </html>
    </web>
    <web id="web_view" name="web.view">
        <html>
            <head>
                <meta charset="utf-8"/>
                <meta name="viewport" content="width=device-width, initial-scale=1"/>
