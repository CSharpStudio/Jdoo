                <script src="/org/jdoo/base/statics/plugins/jquery/jquery.js"></script>
                <script src="/org/jdoo/base/statics/plugins/bootstrap/js/bootstrap.bundle.js"></script>
                <script src="/org/jdoo/base/statics/plugins/select2/js/select2.full.min.js"></script>
                <script src="/org/jdoo/base/statics/plugins/datatables/jquery.dataTables.js"></script>
                <script src="/org/jdoo/base/statics/plugins/datatables-bs4/js/dataTables.bootstrap4.js"></script>
                <script src="/org/jdoo/base/statics/plugins/datatables-responsive/js/dataTables.responsive.js"></script>
                <script src="/org/jdoo/base/statics/plugins/datatables-responsive/js/responsive.bootstrap4.js"></script>
                <script src="/org/jdoo/base/statics/plugins/daterangepicker/daterangepicker.js"></script>
                <script src="/org/jdoo/base/statics/plugins/moment/moment.min.js"></script>
                <script src="/org/jdoo/base/statics/plugins/tempusdominus-bootstrap-4/js/tempusdominus-bootstrap-4.min.js"></script>
                <script src="/org/jdoo/base/statics/plugins/bootstrap-input-spinner/js/bootstrap-input-spinner.js"></script>
                <script src="/org/jdoo/base/statics/dist/js/adminlte.js"></script>
                <script src="/org/jdoo/base/statics/js/src/util.js"></script>
                <script src="/org/jdoo/base/statics/js/src/view.js"></script>
            </head>
            <body class="page-body">
            </body>
        </html>
    </web>
